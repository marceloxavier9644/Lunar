<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="LunarBase" assembly="LunarBase">
	<class name="LunarBase.Classes.BoletoConfig" table="boletoconfig">
		<id name="Id" column="ID" type="int">
			<generator class="native" />
		</id>
		<property name="Descricao" column="DESCRICAO" type="String"/>
		<property name="CodigoBeneficiario" column="CODIGOBENEFICIARIO" type="String" length="20"/>
		<property name="Multa" column="MULTA" type="decimal"/>
		<property name="JuroMensal" column="JUROMENSAL" type="decimal"/>
		<property name="NumeroCarteira" column="NUMEROCARTEIRA" type="String" length="30"/>
		<property name="IdentificacaoCliente" column="IDENTIFICACAOCLIENTE" type="String" length="50"/>
		<property name="Cooperativa" column="COOPERATIVA" type="String" length="10"/>
		<property name="Posto" column="POSTO" type="String" length="10"/>
		<property name="Usuario" column="USUARIO" type="String" length="100"/>
		<property name="Senha" column="SENHA" type="String" length="100"/>
		<property name="IdToken" column="IDTOKEN" type="String" length="100"/>
		<property name="Token" column="TOKEN" type="String" length="500"/>	
		<property name="ContaPadrao" column="CONTAPADRAO" type="Boolean"/>
		<property name="TipoBoleto" column="TIPOBOLETO" type="String" length="100"/>
		<property name="AmbienteProducao" column="AMBIENTEPRODUCAO" type="Boolean"/>
		
		<many-to-one name="ContaBancaria" column="CONTABANCARIA" class="LunarBase.Classes.ContaBancaria" not-null="true" foreign-key="FK_BOLETOCONFIG_CONTABANCARIA" lazy="false"/>
		<many-to-one name="PlanoContaTarifa" column="PLANOCONTATARIFA" class="LunarBase.Classes.PlanoConta" not-null="false" foreign-key="FK_BOLETOCONFIG_PLANOCONTAS" lazy="false"/>


		<!-- Dados do objeto padrão -->
		<property name="DataCadastro" column="DATACADASTRO" type="DateTime"/>
		<property name="DataAlteracao" column="DATAALTERACAO" type="DateTime"/>
		<property name="DataExclusao" column="DATAEXCLUSAO" type="DateTime"/>
		<property name="FlagExcluido" column="FLAGEXCLUIDO" type="Boolean"/>
		<property name="OperadorCadastro" column="OPERADORCADASTRO" type="String" length="20"/>
		<property name="OperadorAlteracao" column="OPERADORALTERACAO" type="String" length="20"/>
		<property name="OperadorExclusao" column="OPERADOREXCLUSAO" type="String" length="20"/>
		<!-- Final dos dados do objeto padrão -->
	</class>
</hibernate-mapping>
