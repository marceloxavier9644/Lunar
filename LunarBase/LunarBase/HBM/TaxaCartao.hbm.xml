<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="LunarBase" assembly="LunarBase">
	<class name="LunarBase.Classes.TaxaCartao" table="taxacartao">
		<id name="Id" column="ID" type="int">
			<generator class="native" />
		</id>
		<property name="Descricao" column="DESCRICAO" type="String" length="150"/>
		<property name="Parcelas" column="PARCELAS" type="int"/>
		<property name="Taxa" column="TAXA" type="double"/>
		<property name="TaxaAdicionalPorParcela" column="TAXAADICIONALPORPARCELA" type="double"/>
		<property name="TarifaAdicional" column="TARIFAADICIONAL" type="decimal"/>
		<property name="DiasRecebimento" column="DIASRECEBIMENTO" type="int"/>
		<property name="Antecipacao" column="ANTECIPACAO" type="Boolean"/>
		<property name="DiasRecebimentoAntecipacao" column="DIASRECEBIMENTOANTECIPACAO" type="int"/>		
		<property name="TaxaAntecipacao" column="TAXAANTECIPACAO" type="double"/>
		<property name="TipoCartao" column="TIPOCARTAO" type="String" length="30"/>

		<many-to-one name="AdquirenteCartao" column="ADQUIRENTECARTAO" class="LunarBase.Classes.AdquirenteCartao" not-null="true" foreign-key="FK_TAXA_ADQUIRENTE" lazy="false"/>
		<many-to-one name="BandeiraCartao" column="BANDEIRACARTAO" class="LunarBase.Classes.BandeiraCartao" not-null="true" foreign-key="FK_TAXA_BANDEIRA" lazy="false"/>
		
		<!-- Dados do objeto padrão -->
		<property name="DataCadastro" column="DATACADASTRO" type="DateTime"/>
		<property name="DataAlteracao" column="DATAALTERACAO" type="DateTime"/>
		<property name="DataExclusao" column="DATAEXCLUSAO" type="DateTime"/>
		<property name="FlagExcluido" column="FLAGEXCLUIDO" type="Boolean"/>
		<property name="OperadorCadastro" column="OPERADORCADASTRO" type="String" length="20"/>
		<property name="OperadorAlteracao" column="OPERADORALTERACAO" type="String" length="20"/>
		<property name="OperadorExclusao" column="OPERADOREXCLUSAO" type="String" length="20"/>
		<!-- Final dos dados do objeto padrão -->
	</class>
</hibernate-mapping>
