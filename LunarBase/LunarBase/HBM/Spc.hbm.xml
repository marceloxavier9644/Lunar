<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="LunarBase" assembly="LunarBase">
	<class name="LunarBase.Classes.Spc" table="spc">
		<id name="Id" column="ID" type="int">
			<generator class="native" />
		</id>
		<property name="NomeUsuario" column="NOMEUSUARIO" type="String" length="100"/>
		<property name="LocalRegistro" column="LOCALREGISTRO" type="String" length="200"/>
		<property name="DataRegistro" column="DATAREGISTRO" type="DateTime"/>
		<property name="ValorRegistro" column="VALORREGISTRO" type="decimal"/>
		<property name="DataConsulta" column="DATACONSULTA" type="DateTime"/>
		<property name="LoginWebService" column="LOGINWEBSERVICE" type="String" length="100"/>
		<property name="QuantidadeRegistro" column="QUANTIDADEREGISTRO" type="double"/>
		
		<many-to-one name="EmpresaFilial" column="EMPRESAFILIAL" class="LunarBase.Classes.EmpresaFilial" not-null="true" foreign-key="FK_SPC_EMPRESA" lazy="false"/>
		<many-to-one name="Pessoa" column="PESSOA" class="LunarBase.Classes.Pessoa" not-null="true" foreign-key="FK_SPC_PESSOA" lazy="false"/>
		
		<!-- Dados do objeto padrão -->
		<property name="DataCadastro" column="DATACADASTRO" type="DateTime"/>
		<property name="DataAlteracao" column="DATAALTERACAO" type="DateTime"/>
		<property name="DataExclusao" column="DATAEXCLUSAO" type="DateTime"/>
		<property name="FlagExcluido" column="FLAGEXCLUIDO" type="Boolean"/>
		<property name="OperadorCadastro" column="OPERADORCADASTRO" type="String" length="20"/>
		<property name="OperadorAlteracao" column="OPERADORALTERACAO" type="String" length="20"/>
		<property name="OperadorExclusao" column="OPERADOREXCLUSAO" type="String" length="20"/>
		<!-- Final dos dados do objeto padrão -->
	</class>
</hibernate-mapping>
