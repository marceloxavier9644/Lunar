<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="LunarBase" assembly="LunarBase">
	<class name="LunarBase.Classes.CaixaAbertura" table="caixaabertura">
		<id name="Id" column="ID" type="int">
			<generator class="native" />
		</id>
		<property name="DataAbertura" column="DATAABERTURA" type="DateTime"/>
		<property name="DataFechamento" column="DATAFECHAMENTO" type="DateTime"/>
		<property name="SaldoInicial" column="SALDOINICIAL" type="decimal"/>
		<property name="SaldoFinal" column="SALDOFINAL" type="decimal"/>
		<property name="DiferencaFechamento" column="DIFERENCAFECHAMENTO" type="decimal"/>
		<property name="Status" column="STATUS" type="String" length="30"/>
		<property name="IdCaixaAnterior" column="IDCAIXAANTERIOR" type="int"/>

		<many-to-one name="EmpresaFilial" column="EMPRESAFILIAL" class="LunarBase.Classes.EmpresaFilial" not-null="true" foreign-key="FK_CAIXAABERTURA_FILIAL" lazy="false"/>
		<many-to-one name="Usuario" column="USUARIO" class="LunarBase.Classes.Usuario" not-null="false" foreign-key="FK_CAIXAABERTURA_USER" lazy="false"/>
		
		<!-- Dados do objeto padrão -->
		<property name="DataCadastro" column="DATACADASTRO" type="DateTime"/>
		<property name="DataAlteracao" column="DATAALTERACAO" type="DateTime"/>
		<property name="DataExclusao" column="DATAEXCLUSAO" type="DateTime"/>
		<property name="FlagExcluido" column="FLAGEXCLUIDO" type="Boolean"/>
		<property name="OperadorCadastro" column="OPERADORCADASTRO" type="String" length="20"/>
		<property name="OperadorAlteracao" column="OPERADORALTERACAO" type="String" length="20"/>
		<property name="OperadorExclusao" column="OPERADOREXCLUSAO" type="String" length="20"/>
		<!-- Final dos dados do objeto padrão -->
	</class>
</hibernate-mapping>
